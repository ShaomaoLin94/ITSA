#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(int argc, char *argv[]) {

    double c;
    while(scanf("%lf", &c)!=EOF){
        double f = round((1.8*c+32)*10)/10;    //(1.8*c+32)是轉成華氏的公式 round(x*10)/10則是四捨五入到小數點第一位的方法
        printf("%.1f\n", f);
    }


	return 0;
}

// c = (f-32)*(5/9)
