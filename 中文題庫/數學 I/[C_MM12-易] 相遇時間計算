#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(int argc, char *argv[]) {

    int distance;
    double speed = 0.238;
    while(scanf("%d", &distance)!=EOF){
        int time = (int)ceil(distance/speed);  //ceil()可以無條件進位至整數，再用int強制轉型
        printf("%d\n", time);
    } 

	return 0;
}
