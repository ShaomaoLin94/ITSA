#include <bits/stdc++.h>
using namespace std;

int main() {
    int power;
    while (cin >> power) {
        if (power > 31) {
            cout << "Value of more than 31" << endl;
        } else {
            int x = 1;
            for (int i = 0; i < power; i++) {
                x = x << 1;
            }
            cout << x << endl;
        }
    }
    return 0;
}
