#include <bits/stdc++.h>
using namespace std;

int main() {
    int num;
    cin >> num;
    int cmp = 128;

    for (int i = 0; i < 8; i++) {
        if (num & cmp) {
            cout << "1";
        } else {
            cout << "0";
        }
        cmp >>= 1;
    }
    cout << endl;

    return 0;
}
