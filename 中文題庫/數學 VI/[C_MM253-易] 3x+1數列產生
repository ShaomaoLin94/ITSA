#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

    int num;
    while(scanf("%d", &num)!=EOF){
        
        printf("%d\n", num);
        while(num>1){
            if(num%2){
                num = num*3+1;
            }
            else{
                num /= 2;
            }
            printf("%d\n", num);
        }
    }



	return 0;
}
