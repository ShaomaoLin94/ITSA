#include <bits/stdc++.h>
using namespace std;

int main(int argc, char *argv[]) {

    int num;
    while (cin >> num) {
        cout << num << endl;
        while (num > 1) {
            if (num % 2) {
                num = num * 3 + 1;
            } else {
                num /= 2;
            }
            cout << num << endl;
        }
    }

    return 0;
}
