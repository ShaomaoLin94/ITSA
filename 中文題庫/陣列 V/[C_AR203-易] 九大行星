#include <bits/stdc++.h>
using namespace std;

int main(int argc, char *argv[]) {

    int test;
    while (cin >> test) {
        while (test--) {
            int num;
            cin >> num;
            switch (num) {
                case 1:
                    cout << "1 Mercury" << endl;
                    break;
                case 2:
                    cout << "2 Venus" << endl;
                    break;
                case 3:
                    cout << "3 Earth" << endl;
                    break;
                case 4:
                    cout << "4 Mars" << endl;
                    break;
                case 5:
                    cout << "5 Juputer" << endl;
                    break;
                case 6:
                    cout << "6 Saturn" << endl;
                    break;
                case 7:
                    cout << "7 Uranus" << endl;
                    break;
                case 8:
                    cout << "8 Neptunus" << endl;
                    break;
                case 9:
                    cout << "9 Pluto" << endl;
                    break;
            }
        }
    }

    return 0;
}
