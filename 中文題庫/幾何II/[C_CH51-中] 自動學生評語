#include <bits/stdc++.h>
using namespace std;

int main(int argc, char *argv[]) {
    int n;
    while (cin >> n) {
        int score;
        while (n--) {
            cin >> score;
            if (score >= 95) {
                cout << "Excellent\n";
            }
            else if (score >= 85) {
                cout << "Very Good\n";
            }
            else if (score >= 61) {
                cout << "Good\n";
            }
            else {
                cout << "Careless\n";
            }
        }
    }
    return 0;
}
