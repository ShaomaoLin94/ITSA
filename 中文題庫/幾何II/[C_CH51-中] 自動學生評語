#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

    int n;
    while(scanf("%d", &n)!=EOF){
        int score;

        while(n--){
            scanf("%d", &score);

            if(score>=95){
                printf("Excellent\n");
            }
            else if(score>=85){
                printf("Very Good\n");
            }
            else if(score>=61){
                printf("Good\n");
            }
            else{
                printf("Careless\n");
            }
        }
    }
    
	return 0;
}
